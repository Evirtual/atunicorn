{
  "name": "atunicorn.io",
  "version": "0.0.0",
  "private": true,
  "description": "quick and simple way to share",
  "main": "index.js",
  "workspaces": {
    "packages": [
      "@packs/**"
    ]
  },
  "scripts": {
    "start": "ENV=test next -p 8080 --webpack",
    "build": "next build --webpack",
    "export": "next build --webpack",
    "serve": "serve -s ./out -p 8080",
    "static": "next build --webpack && yarn serve",
    "deploy": "next build --webpack && firebase deploy --project unicorn-ee877"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.1.10",
    "styled-components": "^6.1.19",
    "firebase": "12.6.0",
    "markdown-to-jsx": "9.3.5",
    "next": "16.0.10",
    "pack": "0.0.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-image-file-resizer": "0.4.8",
    "react-native-web": "0.21.2"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": false
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^2.1.4",
    "firebase-tools": "^15.0.0",
    "next-transpile-modules": "^10.0.1",
    "serve": "^14.2.5"
  },
  "resolutions": {
    "**/minimist": "^1.2.8",
    "**/glob": "^10.5.0",
    "isomorphic-fetch/node-fetch": "2.6.7",
    "**/isomorphic-fetch/node-fetch": "2.6.7",
    "minimatch@3.1.2/brace-expansion": "1.1.12",
    "**/tar": "7.5.2",
    "firebase-tools/js-yaml": "3.14.2",
    "@apidevtools/json-schema-ref-parser/js-yaml": "4.1.1",
    "**/@apidevtools/json-schema-ref-parser/js-yaml": "4.1.1",
    "@modelcontextprotocol/sdk/body-parser": "2.2.1",
    "**/@modelcontextprotocol/sdk/express/body-parser": "2.2.1",
    "**/jws": "^4.0.1",
    "jsonwebtoken/jws": "^3.2.3",
    "**/@modelcontextprotocol/sdk": "^1.24.0"
  },
  "author": "",
  "license": "ISC"
}
