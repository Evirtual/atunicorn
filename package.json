{
  "name": "atunicorn.io",
  "version": "0.0.0",
  "private": true,
  "description": "quick and simple way to share",
  "main": "index.js",
  "workspaces": {
    "packages": [
      "@packs/**"
    ]
  },
  "scripts": {
    "start": "ENV=test next -p 8080 --webpack",
    "build": "next build --webpack",
    "export": "next build --webpack",
    "serve": "serve -s ./out -p 8080",
    "static": "next build --webpack && yarn serve",
    "deploy": "next build --webpack && firebase deploy --project unicorn-ee877"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.1.10",
    "styled-components": "^6.1.19",
    "firebase": "12.6.0",
    "markdown-to-jsx": "9.3.5",
    "next": "16.0.10",
    "pack": "0.0.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-image-file-resizer": "0.4.8",
    "react-native-web": "0.21.2"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": false
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^2.1.4",
    "firebase-tools": "^15.0.0",
    "next-transpile-modules": "^10.0.1",
    "serve": "^14.2.5"
  },
  "author": "",
  "license": "ISC"
}
