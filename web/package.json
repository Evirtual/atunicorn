{
  "name": "atunicorn.io",
  "version": "0.0.0",
  "description": "quick and simple way to share",
  "scripts": {
    "start": "ENV=test next -p 8080",
    "build": "next build",
    "export": "next export",
    "serve": "serve ./out -p 8080",
    "static": "npm run build && npm run export && npm run serve",
    "deploy": "npm run build && npm run export && firebase deploy --project unicorn-ee877"
  },
  "dependencies": {
    "pack": "0.0.0",
    "@react-native-async-storage/async-storage": "1.17.0",
    "actheme": "0.0.36",
    "actstore": "0.0.38",
    "fetchier": "0.0.36",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native-web": "0.17.7",
    "@fortawesome/fontawesome-svg-core": "1.2.34",
    "@fortawesome/pro-duotone-svg-icons": "5.15.2",
    "@fortawesome/pro-light-svg-icons": "5.15.2",
    "@fortawesome/pro-regular-svg-icons": "5.15.2",
    "@fortawesome/pro-solid-svg-icons": "5.15.2",
    "@fortawesome/free-brands-svg-icons": "5.15.2",
    "@fortawesome/react-fontawesome": "0.1.14",
    "firebase": "8.2.10",
    "react-image-file-resizer": "0.4.7",
    "markdown-to-jsx": "7.1.7"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "devDependencies": {
    "next-transpile-modules": "^9.0.0",
    "serve": "^13.0.2"
  }
}
