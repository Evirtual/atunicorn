    "deploy": "npm run build && npm run export && firebase deploy --project unicorn-ee877"
  },
  "dependencies": {
    "pack": "0.0.0",
    "@react-native-async-storage/async-storage": "1.13.2",
    "actheme": "0.0.34",
    "actstore": "0.0.34",
    "crypto-js": "^4.0.0",
    "fetchier": "0.0.36",
    "next": "^10.0.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "rnwc": "^0.13.0",
    "@fortawesome/fontawesome-svg-core": "1.2.34",
    "@fortawesome/pro-duotone-svg-icons": "5.15.2",
    "@fortawesome/pro-light-svg-icons": "5.15.2",
    "@fortawesome/pro-regular-svg-icons": "5.15.2",
    "@fortawesome/pro-solid-svg-icons": "5.15.2",
    "@fortawesome/free-brands-svg-icons": "5.15.2",
    "@fortawesome/react-fontawesome": "0.1.14",
    "firebase": "8.2.10"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "devDependencies": {
    "next-transpile-modules": "^6.0.0",
    "serve": "^11.3.2"
  }
