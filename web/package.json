{
  "name": "atunicorn.io",
  "version": "0.0.0",
  "description": "quick and simple way to share",
  "scripts": {
    "start": "ENV=test next -p 8080",
    "build": "next build",
    "export": "next export",
    "serve": "serve ./out -p 8080",
